extends layout

block content
    include inner
    div
        img.my-zip-img(src="../../public/images/results_banner3.png")
        div.container
            div.main-wrapper
                div.row.row-top
                    div.col-lg-6
                      h4 NORTH AMERICA
                      each contact in data
                       h2 #{contact['city']}, 
                         span #{contact.stateAcronym}  #{contact.zipCode}
                       p= contact.mayorGHGTarget
                       - break
                div.row.row-top
                    div.col-lg-11.progress-column
                      div.select-progress 
                       a.progress-link.active#state-progress State 
                       a.progress-link - 
                       a.progress-link#country-progress Country
                      div.progress.progress-animation(data-value=`${percentageGovernor}` data-country=`${percentageCountry}` data-state=`${percentageGovernor}`)
                        div(class="progress-bar" role="progressbar" aria-valuenow=`${percentageCountry}` style="width:0;" aria-valuemin="0" aria-valuemax="100")
                        h6 
                          strong
                            span.progress-difference 100% 
                            | away from target
                    div.custom-tooltip.center-tooltip ?
                      span.custom-tooltiptext
                        a(href="/myState") How do I compare to other places?
                div.row.row-main
                  div.col-lg-6
                    h2.page-title Who's in charge?
                div.row.row-main
                    div.col-lg-12
                      div.results-list
                        div.row
                         each contact in data
                          div.col-lg-4
                            div.col-item
                              div.item-img
                                div.avatar-container
                                  if contact.repPicture
                                    img(src=`../../public/images/${contact.repPicture}.jpg`)
                                  else
                                    img(src="../../public/images/placeholder-rep.png")
                              div.item-data
                                h5 
                                  svg.bi-geo-alt(width='13px' height='13px' viewbox='0 0 16 16' fill='currentColor' xmlns='http://www.w3.org/2000/svg')
                                    path(fill-rule='evenodd' d='M12.166 8.94C12.696 7.867 13 6.862 13 6A5 5 0 0 0 3 6c0 .862.305 1.867.834 2.94.524 1.062 1.234 2.12 1.96 3.07A31.481 31.481 0 0 0 8 14.58l.208-.22a31.493 31.493 0 0 0 1.998-2.35c.726-.95 1.436-2.008 1.96-3.07zM8 16s6-5.686 6-10A6 6 0 0 0 2 6c0 4.314 6 10 6 10z')
